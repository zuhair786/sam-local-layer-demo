https://adamnovotny.com/blog/serving-dynamic-web-pages-using-python-and-aws-lambda.html


PythonDepLayer:
    Type: AWS::Serverless::LayerVersion
    Properties:
      LayerName: MyLambdaLayer
      Description: My Lambda Layer with jinja2
      ContentUri: lambda-layers/python.zip
      CompatibleRuntimes: [python3.9] # (OR) S3://location
      RetentionPolicy: Retain

Layers: 
    - !Ref PythonDepLayer


zip-files -a -o python.zip ./layer1 ./layer1-1.0.dist-info


